---
import DefaultLayout from '@layouts/default.astro'

const { title } = Astro.props
---

<DefaultLayout title={title}>
	<slot />
	<slot slot='head-start' name='head-start' />
	<slot slot='head-end' name='head-end' />
	<slot slot='body-start' name='body-start' />
	<slot slot='body-end' name='body-end' />
	<slot slot='header' name='header' />
	<slot slot='footer' name='footer' />
</DefaultLayout>
